{
  "regexManagers": [
    {
      "fileMatch": [
        "^README.md$",
        "^\\.github/workflows/.+\\.ya?ml$"
      ],
      "matchStrings": [
        "uses: golangci/golangci-lint-action@.+?\\s+with:\\s+version: (?<currentValue>.+)\\n"
      ],
      "depNameTemplate": "golangci/golangci-lint",
      "datasourceTemplate": "github-releases"
    }
  ]
}
